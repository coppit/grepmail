t/mailarc-1.txt:136:
t/mailarc-1.txt:137:#define MOTOR     0
t/mailarc-1.txt:138:#define AO        1
t/mailarc-1.txt:139:#define ANALOG    2
t/mailarc-1.txt:140:#define DIGITAL   3
t/mailarc-1.txt:141:#define PRINTF    4
t/mailarc-1.txt:142:#define KNOB      5
t/mailarc-1.txt:143:#define BEEP      6
t/mailarc-1.txt:144:#define TONE      7
t/mailarc-1.txt:145:#define START_BUTTON 8
t/mailarc-1.txt:146:#define STOP_BUTTON  9
t/mailarc-1.txt:147:#define QUIT    113
t/mailarc-1.txt:148:
t/mailarc-1.txt:149:#define sleep(NUM) _sleep(NUM)
t/mailarc-1.txt:150:#define SERIALWAIT  5
t/mailarc-1.txt:151:
t/mailarc-1.txt:152:unsigned short PORT = 0x3f8; // LPT1: 0x378 COM1: 0x3f8
t/mailarc-1.txt:153:
t/mailarc-1.txt:154:int send(int i) {
t/mailarc-1.txt:155:        int retval;
t/mailarc-1.txt:156:        retval = _outp( PORT, i);
t/mailarc-1.txt:157:        _sleep(SERIALWAIT);
t/mailarc-1.txt:158:        return retval;
t/mailarc-1.txt:159:}
t/mailarc-1.txt:160:
t/mailarc-1.txt:161:int receive() {
t/mailarc-1.txt:162:        int retval;
t/mailarc-1.txt:163:        retval = _inp( PORT);
t/mailarc-1.txt:164:        _sleep(SERIALWAIT);
t/mailarc-1.txt:165:        retval = _inp( PORT);
t/mailarc-1.txt:166:        return retval;
t/mailarc-1.txt:167:}
t/mailarc-1.txt:168:
t/mailarc-1.txt:169:void hangup() {
t/mailarc-1.txt:170:        send(QUIT);
t/mailarc-1.txt:171:}
t/mailarc-1.txt:172:
t/mailarc-1.txt:173:void print(char buffer[]) {
t/mailarc-1.txt:174:        int i;
t/mailarc-1.txt:175:        send(PRINTF);
t/mailarc-1.txt:176:        for (i = 0; buffer[i] != 0; i++)
t/mailarc-1.txt:177:                send(buffer[i]);
t/mailarc-1.txt:178:        send('\0');
t/mailarc-1.txt:179:}
t/mailarc-1.txt:180:
t/mailarc-1.txt:181:void motor(int motornum, int power) {
t/mailarc-1.txt:182:        send(MOTOR);
t/mailarc-1.txt:183:        send(motornum);
t/mailarc-1.txt:184:        send(power + 100); // taken off on the other end
t/mailarc-1.txt:185:}
t/mailarc-1.txt:186:
t/mailarc-1.txt:187:int analog(int sensor) {
t/mailarc-1.txt:188:        send(ANALOG);
t/mailarc-1.txt:189:        send(sensor);
t/mailarc-1.txt:190:        return receive();
t/mailarc-1.txt:191:}
t/mailarc-1.txt:192:
t/mailarc-1.txt:193:int digital(int sensor) {
t/mailarc-1.txt:194:        send(DIGITAL);
t/mailarc-1.txt:195:        send(sensor);
t/mailarc-1.txt:196:        return receive();
t/mailarc-1.txt:197:}
t/mailarc-1.txt:198:
t/mailarc-1.txt:199:void ao() {
t/mailarc-1.txt:200:        send(AO);
t/mailarc-1.txt:201:}
t/mailarc-1.txt:202:
t/mailarc-1.txt:203:int knob() {
t/mailarc-1.txt:204:        send(KNOB);
t/mailarc-1.txt:205:        return receive();
t/mailarc-1.txt:206:}
t/mailarc-1.txt:207:
t/mailarc-1.txt:208:void beep() {
t/mailarc-1.txt:209:        send(BEEP);
t/mailarc-1.txt:210:}
t/mailarc-1.txt:211:
t/mailarc-1.txt:212:void tone(char f1[], char f2[]) {
t/mailarc-1.txt:213:        int i;
t/mailarc-1.txt:214:        send(TONE);
t/mailarc-1.txt:215:        for (i = 0; f1[i] != 0; i++)
t/mailarc-1.txt:216:                send(f1[i]);
t/mailarc-1.txt:217:        send('\0');
t/mailarc-1.txt:218:        for (i = 0; f2[i] != 0; i++)
t/mailarc-1.txt:219:                send(f2[i]);
t/mailarc-1.txt:220:        send('\0');
t/mailarc-1.txt:221:        _sleep((unsigned long) (atof(f2) * 1000)); // to keep from
t/mailarc-1.txt:222:overflowing serial line
t/mailarc-1.txt:223:}
t/mailarc-1.txt:224:
t/mailarc-1.txt:225:void interactive()
t/mailarc-1.txt:226:{
t/mailarc-1.txt:227:        char c;
t/mailarc-1.txt:228:        char key = ' ';
t/mailarc-1.txt:229:        while (key != 'q') {
t/mailarc-1.txt:230:                key = getch();
t/mailarc-1.txt:231:                send(key);
t/mailarc-1.txt:232:                printf("Sent %c\n", key);
t/mailarc-1.txt:233:                c = receive();
t/mailarc-1.txt:234:                printf("Got %c as a return value\n", c);
t/mailarc-1.txt:235:        }
t/mailarc-1.txt:236:}
t/mailarc-1.txt:237:
t/mailarc-1.txt:238:int start_button() {
t/mailarc-1.txt:239:        send(START_BUTTON);
t/mailarc-1.txt:240:        return receive();
t/mailarc-1.txt:241:}
t/mailarc-1.txt:242:
t/mailarc-1.txt:243:int stop_button() {
t/mailarc-1.txt:244:        send(STOP_BUTTON);
t/mailarc-1.txt:245:        return receive();
t/mailarc-1.txt:246:}
t/mailarc-1.txt:247://************** END: serial_HOST.h
