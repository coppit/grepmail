t/mailarc-1.txt:258:*/
t/mailarc-1.txt:259:
t/mailarc-1.txt:260:#define MOTOR     0
t/mailarc-1.txt:261:#define AO        1
t/mailarc-1.txt:262:#define ANALOG    2
t/mailarc-1.txt:263:#define DIGITAL   3
t/mailarc-1.txt:264:#define PRINTF    4
t/mailarc-1.txt:265:#define KNOB      5
t/mailarc-1.txt:266:#define BEEP    6
t/mailarc-1.txt:267:#define TONE    7
t/mailarc-1.txt:268:#define START_BUTTON 8
t/mailarc-1.txt:269:#define STOP_BUTTON  9
t/mailarc-1.txt:270:#define QUIT    113
t/mailarc-1.txt:271:
t/mailarc-1.txt:272:int _isspace(int a)         /* returns 1 for space or tab, 0
t/mailarc-1.txt:273:otherwise     */
t/mailarc-1.txt:274:                            /* internal routine used by atof() and
t/mailarc-1.txt:275:cgets() */
t/mailarc-1.txt:276:
t/mailarc-1.txt:277:{
t/mailarc-1.txt:278:    return ((a == 32) || (a == 9));     /* 32 is space, 9 is tab */
t/mailarc-1.txt:279:}
t/mailarc-1.txt:280:
t/mailarc-1.txt:281:/*****************************************************************************/
t/mailarc-1.txt:282:
t/mailarc-1.txt:283:int _isdigit(int a)         /* returns 1 if a digit 0-9, 0 otherwise */
t/mailarc-1.txt:284:                            /* internal routine used by atof()       */
t/mailarc-1.txt:285:
t/mailarc-1.txt:286:{
t/mailarc-1.txt:287:    return ((a >= 48) && (a <= 57));    /* 48 is '0', 57 is '9' */
t/mailarc-1.txt:288:}
t/mailarc-1.txt:289:
t/mailarc-1.txt:290:float atof(char s[])    /* Convert a string containing a number in
t/mailarc-1.txt:291:ASCII     */
t/mailarc-1.txt:292:                        /* form (integer, float, or exponential float)
t/mailarc-1.txt:293:to a  */
t/mailarc-1.txt:294:                        /* float.  Strips whitespace characters (space
t/mailarc-1.txt:295:and   */
t/mailarc-1.txt:296:                        /* tab) from the front of the string, but
t/mailarc-1.txt:297:stops      */
t/mailarc-1.txt:298:                        /* parsing at the first (unexpected)
t/mailarc-1.txt:299:non-numeric     */
t/mailarc-1.txt:300:                        /* character if the string has garbage at the
t/mailarc-1.txt:301:end.   */
t/mailarc-1.txt:302:                        /* This means that "  34.3foo78" translates to
t/mailarc-1.txt:303:34.3. */
t/mailarc-1.txt:304:                        /* Modified from atof() function in the
t/mailarc-1.txt:305:standard     */
t/mailarc-1.txt:306:                        /* library of the Hi-Tec C compiler for
t/mailarc-1.txt:307:CP/M.        */
t/mailarc-1.txt:308:                        /* Note:  all string literals converted to
t/mailarc-1.txt:309:decimal   */
t/mailarc-1.txt:310:                        /* form because IC can't deal with string
t/mailarc-1.txt:311:literals   */
t/mailarc-1.txt:312:                        /* in math
t/mailarc-1.txt:313:calculations.                             */
t/mailarc-1.txt:314:                        /* Also note:  very ugly code because IC will
t/mailarc-1.txt:315:not    */
t/mailarc-1.txt:316:                        /* allow any math operations on pointers!  Thus,
t/mailarc-1.txt:317:the */
t/mailarc-1.txt:318:                        /* the number string has to be treated as an
t/mailarc-1.txt:319:array!  */
t/mailarc-1.txt:320:                        /* Also also note:  no error handling; assumes
t/mailarc-1.txt:321:that  */
t/mailarc-1.txt:322:                        /* the string is a valid representation of a
t/mailarc-1.txt:323:number! */
t/mailarc-1.txt:324:                        /* Valid range for exponential-format numbers
t/mailarc-1.txt:325:is     */
t/mailarc-1.txt:326:                        /* approximately 2.0e-38 to
t/mailarc-1.txt:327:3.4e+38.                 */
t/mailarc-1.txt:328:
t/mailarc-1.txt:329:{
t/mailarc-1.txt:330:    int     i=0;            /* index into string array */
t/mailarc-1.txt:331:    int     sign=0;         /* mantissa sign flag:  0=positive,
t/mailarc-1.txt:332:1=negative */
t/mailarc-1.txt:333:    int     exp0=0;         /* mantissa exponent counter */
t/mailarc-1.txt:334:    int     eexp=0;         /* E-form exponent counter */
t/mailarc-1.txt:335:    int     expsign=0;      /* exponent sign flag:  0=positive,
t/mailarc-1.txt:336:1=negative */
t/mailarc-1.txt:337:    float   m=0.0;          /* mantissa accumulator */
t/mailarc-1.txt:338:
t/mailarc-1.txt:339:    /* skip any leading whitespace (space, tab) */
t/mailarc-1.txt:340:    while (_isspace(s[i]))
t/mailarc-1.txt:341:        i++;                                /* skip it */
t/mailarc-1.txt:342:
t/mailarc-1.txt:343:    /* check for mantissa sign */
t/mailarc-1.txt:344:    if (s[i] == 45)                         /* 45 is '-' */
t/mailarc-1.txt:345:    {
t/mailarc-1.txt:346:        sign = 1;                           /* flag minus sign */
t/mailarc-1.txt:347:        i++;                                /* point to next */
t/mailarc-1.txt:348:    }
t/mailarc-1.txt:349:    else if (s[i] == 43)                    /* 43 is '+' */
t/mailarc-1.txt:350:        i++;                                /* point to next */
t/mailarc-1.txt:351:
t/mailarc-1.txt:352:    /* now get all digits up to either a decimal point or an e/E */
t/mailarc-1.txt:353:    while (_isdigit(s[i]))
t/mailarc-1.txt:354:    {
t/mailarc-1.txt:355:        m = 10.0*m + (float)(s[i] - 48);    /* 48 is '0' */
t/mailarc-1.txt:356:        i++;                                /* point to next */
t/mailarc-1.txt:357:    }
t/mailarc-1.txt:358:
t/mailarc-1.txt:359:    /* no more digits, so check for decimal point */
t/mailarc-1.txt:360:    if (s[i] == 46)                         /* 46 is '.' */
t/mailarc-1.txt:361:    {
t/mailarc-1.txt:362:        i++;                                /* point to next */
t/mailarc-1.txt:363:        /* get all digits after decimal point */
t/mailarc-1.txt:364:        while (_isdigit(s[i]))
t/mailarc-1.txt:365:        {
t/mailarc-1.txt:366:            exp0--;
t/mailarc-1.txt:367:            m = 10.0*m + (float)(s[i] - 48);    /* 48 is '0' */
t/mailarc-1.txt:368:            i++;                                /* point to next */
t/mailarc-1.txt:369:        }
t/mailarc-1.txt:370:    }
t/mailarc-1.txt:371:
t/mailarc-1.txt:372:    /* check for e/E exponential form */
t/mailarc-1.txt:373:    if ((s[i] == 101) || (s[i] == 69))      /* 101 is 'e', 69 is 'E' */
t/mailarc-1.txt:374:    {
t/mailarc-1.txt:375:        i++;                                /* point to next */
t/mailarc-1.txt:376:        /* check for exponent sign */
t/mailarc-1.txt:377:        if (s[i] == 45)                     /* 45 is '-' */
t/mailarc-1.txt:378:        {
t/mailarc-1.txt:379:            expsign = 1;                    /* flag negative exponent */
t/mailarc-1.txt:380:            i++;                            /* point to next */
t/mailarc-1.txt:381:        }
t/mailarc-1.txt:382:        else if (s[i] == 43)                /* 43 is '+' */
t/mailarc-1.txt:383:            i++;                            /* point to next */
t/mailarc-1.txt:384:
t/mailarc-1.txt:385:        /* now get exponent */
t/mailarc-1.txt:386:        while (_isdigit(s[i]))
t/mailarc-1.txt:387:        {
t/mailarc-1.txt:388:            eexp = eexp*10 + s[i] - 48;     /* 48 is '0' */
t/mailarc-1.txt:389:            i++;                            /* point to next */
t/mailarc-1.txt:390:        }
t/mailarc-1.txt:391:
t/mailarc-1.txt:392:        /* adjust exponent sign */
t/mailarc-1.txt:393:        if (expsign)
t/mailarc-1.txt:394:            eexp = -eexp;                   /* make it negative */
t/mailarc-1.txt:395:    }
t/mailarc-1.txt:396:
t/mailarc-1.txt:397:    /* compute absolute value of final float */
t/mailarc-1.txt:398:    exp0 += eexp;
t/mailarc-1.txt:399:    while (exp0 < 0)                    /* for negative exponents */
t/mailarc-1.txt:400:    {
t/mailarc-1.txt:401:        m = m / 10.0;
t/mailarc-1.txt:402:        exp0++;
t/mailarc-1.txt:403:    }
t/mailarc-1.txt:404:    while (exp0 > 0)                    /* for positive exponents */
t/mailarc-1.txt:405:    {
t/mailarc-1.txt:406:        m = m * 10.0;
t/mailarc-1.txt:407:        exp0--;
t/mailarc-1.txt:408:    }
t/mailarc-1.txt:409:
t/mailarc-1.txt:410:    /* adjust final float sign from mantissa */
t/mailarc-1.txt:411:    if (sign)
t/mailarc-1.txt:412:        return (-m);                    /* negative */
t/mailarc-1.txt:413:    else
t/mailarc-1.txt:414:        return (m);                     /* positive */
t/mailarc-1.txt:415:}
t/mailarc-1.txt:416:
t/mailarc-1.txt:417:void disable_pcode_serial()
t/mailarc-1.txt:418:/* necessary to receive characters using serial_getchar */
t/mailarc-1.txt:419:{
t/mailarc-1.txt:420:   poke(0x3c, 1);
t/mailarc-1.txt:421:}
t/mailarc-1.txt:422:
t/mailarc-1.txt:423:void reenable_pcode_serial()
t/mailarc-1.txt:424:/* necessary for IC to interact with board again */
t/mailarc-1.txt:425:{
t/mailarc-1.txt:426:   poke(0x3c, 0);
t/mailarc-1.txt:427:}
t/mailarc-1.txt:428:
t/mailarc-1.txt:429:/*
t/mailarc-1.txt:430:======================================================================
t/mailarc-1.txt:431:For sending and receiving single bytes, you can use Randy's IC code:
t/mailarc-1.txt:432:*/
t/mailarc-1.txt:433:
t/mailarc-1.txt:434:void serial_putchar(int c)
t/mailarc-1.txt:435:{
t/mailarc-1.txt:436:   while (!(peek(0x102e) & 0x80));  /* wait until serial transmit empty
t/mailarc-1.txt:437:*/
t/mailarc-1.txt:438:   poke(0x102f, c);  /* send character */
t/mailarc-1.txt:439:}
t/mailarc-1.txt:440:
t/mailarc-1.txt:441:int serial_getchar()
t/mailarc-1.txt:442:{
t/mailarc-1.txt:443:   while (!(peek(0x102e) & 0x20)); /* wait for received character */
t/mailarc-1.txt:444:   return peek(0x102f);
t/mailarc-1.txt:445:}
t/mailarc-1.txt:446:
t/mailarc-1.txt:447:void main(void) {
t/mailarc-1.txt:448:        int pos, c = ' ', var1, var2;
t/mailarc-1.txt:449:        float f1, f2;
t/mailarc-1.txt:450:        char buffer[80];
t/mailarc-1.txt:451:        disable_pcode_serial();
t/mailarc-1.txt:452:        beep();
t/mailarc-1.txt:453:        printf("\nSerial IO Mode!");
t/mailarc-1.txt:454:        printf("Listening...");
t/mailarc-1.txt:455:        msleep(500L);
t/mailarc-1.txt:456:        while (c != 'q') {
t/mailarc-1.txt:457:                c = serial_getchar();
t/mailarc-1.txt:458:/*              printf("[%d] ", c); */
t/mailarc-1.txt:459:                if (c == MOTOR) {
t/mailarc-1.txt:460:                        var1 = serial_getchar();
t/mailarc-1.txt:461:                        var2 = serial_getchar() - 100;
t/mailarc-1.txt:462:                        motor(var1, var2);
t/mailarc-1.txt:463:                } else if (c == AO) {
t/mailarc-1.txt:464:                        ao();
t/mailarc-1.txt:465:                } else if (c == ANALOG) {
t/mailarc-1.txt:466:                        var1 = serial_getchar();
t/mailarc-1.txt:467:                        serial_putchar(analog(var1));
t/mailarc-1.txt:468:                } else if (c == DIGITAL) {
t/mailarc-1.txt:469:                        var1 = serial_getchar();
t/mailarc-1.txt:470:                        serial_putchar(digital(var1));
t/mailarc-1.txt:471:                } else if (c == PRINTF) {
t/mailarc-1.txt:472:                        pos = 0;
t/mailarc-1.txt:473:                        while (c != 0) {
t/mailarc-1.txt:474:                                buffer[pos++] = c;
t/mailarc-1.txt:475:                                c = serial_getchar();
t/mailarc-1.txt:476:                        }
t/mailarc-1.txt:477:                        buffer[pos] = '\0';
t/mailarc-1.txt:478:                        printf(buffer);
t/mailarc-1.txt:479:                } else if (c == TONE) {
t/mailarc-1.txt:480:                        pos = 0;
t/mailarc-1.txt:481:                        c = serial_getchar();
t/mailarc-1.txt:482:                        while (c != 0) {
t/mailarc-1.txt:483:                                buffer[pos++] = c;
t/mailarc-1.txt:484:                                c = serial_getchar();
t/mailarc-1.txt:485:                        }
t/mailarc-1.txt:486:                        buffer[pos] = '\0';
t/mailarc-1.txt:487:                        f1 = atof(buffer);
t/mailarc-1.txt:488:                        pos = 0;
t/mailarc-1.txt:489:                        c = serial_getchar();
t/mailarc-1.txt:490:                        while (c != 0) {
t/mailarc-1.txt:491:                                buffer[pos++] = c;
t/mailarc-1.txt:492:                                c = serial_getchar();
t/mailarc-1.txt:493:                        }
t/mailarc-1.txt:494:                        buffer[pos] = '\0';
t/mailarc-1.txt:495:                        f2 = atof(buffer);
t/mailarc-1.txt:496:                        tone(f1, f2);
t/mailarc-1.txt:497:                } else if (c == START_BUTTON) {
t/mailarc-1.txt:498:                        serial_putchar(start_button());
t/mailarc-1.txt:499:                } else if (c == STOP_BUTTON) {
t/mailarc-1.txt:500:                        serial_putchar(stop_button());
t/mailarc-1.txt:501:                } else if (c == BEEP) {
t/mailarc-1.txt:502:                        beep();
t/mailarc-1.txt:503:                } else if (c == KNOB) {
t/mailarc-1.txt:504:                        serial_putchar(knob());
t/mailarc-1.txt:505:                }
t/mailarc-1.txt:506:      }
t/mailarc-1.txt:507:        reenable_pcode_serial();
t/mailarc-1.txt:508:        printf("\nHB Mode!");
t/mailarc-1.txt:509:}
t/mailarc-1.txt:510:
t/mailarc-1.txt:511://************** END: serial_HB.c
